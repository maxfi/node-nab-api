{
  "version": "1.0.0-rc.1",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "docs:generate": "npx typedoc src/index.ts",
    "docs:serve": "yarn run docs:generate && npx serve docs",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "publish": "npx np"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "node-nab-api",
  "author": "Max",
  "module": "dist/node-nab-api.esm.js",
  "size-limit": [
    {
      "path": "dist/node-nab-api.cjs.production.min.js",
      "limit": "40 KB"
    },
    {
      "path": "dist/node-nab-api.esm.js",
      "limit": "40 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.12.0",
    "@types/node-fetch": "^2.5.10",
    "husky": "^6.0.0",
    "size-limit": "^4.12.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "date-fns": "^2.22.1",
    "is-plain-object": "^5.0.0",
    "node-fetch": "^2.6.7",
    "xml-js": "^1.6.11"
  }
}
