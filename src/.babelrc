{
  "env": {
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "4"
            },
            "modules": false,
            "useBuiltIns": "usage",
            "debug": true
          },
          "@babel/preset-flow"
        ]
      ],
      "plugins": ["@babel/plugin-transform-runtime"]
    },
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "usage"
          },
          "@babel/preset-flow"
        ]
      ]
    }
  },
  "plugins": ["@babel/plugin-proposal-object-rest-spread"]
}
